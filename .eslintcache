load: res.data,\n        });\n      })\n      .catch((err) => {\n        dispatch({\n          type: \"FETCH_ERROR\",\n        });\n      });\n  }, []);\n\n  return (\n    <div>\n      <h1>useReducer Fetch 2</h1>\n      {state.loading ? <p>Loading...</p> : null}\n      {state.error ? <p>{state.error}</p> : null}\n      {state.post ? <p>{state.post.title}</p> : null}\n    </div>\n  );\n}\n\nexport default ReducerFetch2;\n","/home/ak/Desktop/react/react-hooks/src/app/useCallBackHook/ParentCallBack.js",["162","163"],"import React, { useCallback, useState } from \"react\";\nimport Button from \"./Button\";\nimport Count from \"./Count\";\nimport Title from \"./Title\";\n\nfunction ParentCallBack() {\n  const [age, setAge] = useState(25);\n  const [salary, setSalary] = useState(50000);\n\n  const incrementAge = useCallback(() => {\n    setAge((prev) => prev + 1);\n  }, [age]);\n\n  const incrementSalary = useCallback(() => {\n    setSalary((prev) => prev + 1000);\n  }, [salary]);\n\n  return (\n    <div>\n      <Title />\n      <Count text=\"age\" count={age} />\n      <Button handleClick={incrementAge} title=\"Increment age\" />\n      <Count text=\"Salary\" count={salary} />\n      <Button handleClick={incrementSalary} title=\"Increment Salary\" />\n    </div>\n  );\n}\n\nexport default ParentCallBack;\n","/home/ak/Desktop/react/react-hooks/src/app/useCallBackHook/Title.js",[],"/home/ak/Desktop/react/react-hooks/src/app/useCallBackHook/Count.js",[],"/home/ak/Desktop/react/react-hooks/src/app/useCallBackHook/Button.js",[],"/home/ak/Desktop/react/react-hooks/src/app/Component/useMemoHook.js",["164"],"import React, { useMemo, useState } from \"react\";\n\nfunction UseMemoHook() {\n  const [countOne, setCountOne] = useState(0);\n  const [countTwo, setCountTwo] = useState(0);\n\n  const isEvent = useMemo(() => {\n    let i = 0;\n    while (i < 200000000) i++;\n    return countOne % 2 == 0;\n  }, [countOne]);\n\n  return (\n    <div>\n      <div>\n        <button onClick={()