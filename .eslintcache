\">\n          Counter Two - {countTwo}\n        </button>\n      </div>\n    </div>\n  );\n}\n\nexport default UseMemoHook;\n","/home/ak/Desktop/react/react-hooks/src/app/UseRef/UseRefOne.js",[],{"ruleId":"160","replacedBy":"161"},{"ruleId":"162","replacedBy":"163"},{"ruleId":"164","severity":1,"message":"165","line":2,"column":10,"nodeType":"166","messageId":"167","endLine":2,"endColumn":23},{"ruleId":"164","severity":1,"message":"168","line":2,"column":25,"nodeType":"166","messageId":"167","endLine":2,"endColumn":30},{"ruleId":"164","severity":1,"message":"169","line":2,"column":32,"nodeType":"166","messageId":"167","endLine":2,"endColumn":38},{"ruleId":"164","severity":1,"message":"170","line":3,"column":8,"nodeType":"166","messageId":"167","endLine":3,"endColumn":16},{"ruleId":"164","severity":1,"message":"171","line":4,"column":8,"nodeType":"166","messageId":"167","endLine":4,"endColumn":20},{"ruleId":"164","severity":1,"message":"172","line":5,"column":8,"nodeType":"166","messageId":"167","endLine":5,"endColumn":23},{"ruleId":"164","severity":1,"message":"173","line":6,"column":8,"nodeType":"166","messageId":"167","endLine":6,"endColumn":19},{"ruleId":"164","severity":1,"message":"174","line":7,"column":8,"nodeType":"166","messageId":"167","endLine":7,"endColumn":22},{"ruleId":"164","severity":1,"message":"175","line":8,"column":8,"nodeType":"166","messageId":"167","endLine":8,"endColumn":17},{"ruleId":"164","severity":1,"message":"176","line":9,"column":8,"nodeType":"166","messageId":"167","endLine":9,"endColumn":16},{"ruleId":"164","severity":1,"message":"177","line":10,"column":8,"nodeType":"166","messageId":"167","endLine":10,"endColumn":20},{"ruleId":"164","severity":1,"message":"178","line":11,"column":8,"nodeType":"166","messageId":"167","endLine":11,"endColumn":23},{"ruleId":"164","severity":1,"message":"179","line":12,"column":8,"nodeType":"166","messageId":"167","endLine":12,"endColumn":23},{"ruleId":"164","severity":1,"message":"180","line":13,"col